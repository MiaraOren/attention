CREATE TABLE Students (
    ID VARCHAR(50) NOT NULL,
    username VARCHAR(30) NOT NULL,
    salt VARCHAR(64) NOT NULL,
    pass VARCHAR(64) NOT NULL,
    date_created datetime,
    PRIMARY KEY (ID)
);


